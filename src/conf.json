{
	"use": ["log"],
	"attr":{
		"default": {
			"writable"     : false,
			"configurable" : false,
			"enumerable"   : true
		},

		"modules": {
			"__test_valid": { "writable": true }
		}
	},
	"log":{
		"colors" : true,
		"use"    : ".*",
		"attr"   : {
			"enumerable"   : true,
			"writable"     : false,
			"configurable" : false
		}
	}
}